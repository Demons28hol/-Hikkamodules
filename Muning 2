# meta developer: @MisterHolY
from asyncio import sleep

from .. import loader


@loader.tds
class muning(loader.Module):
    strings = {"name": "muning"}

    @loader.owner
    async def muningcmd(self, message):
        """.используй чтобы сделать мьюнинг в telegram."""
        for _ in range(1):
            for muning in [
                "🤫",
                "🧏‍♂️",                                                
                "BAY"
                "BAY!"

            ]:
                await message.edit(muning)
                await sleep(1.3)
